<sql name="SQL 1*">SELECT
    a.month,
    SUM(a.revenue) AS actual_revenue,
    SUM(b.budget_revenue) AS budget_revenue,
    SUM(a.revenue) - SUM(b.budget_revenue) AS revenue_variance,
    ROUND(100.0 * (SUM(a.revenue) - SUM(b.budget_revenue)) / SUM(b.budget_revenue), 2) AS variance_pct,
    SUM(a.revenue - a.cogs - a.opex) AS operating_profit
FROM actuals a
JOIN budget b
    ON a.month = b.month
    AND a.region = b.region
    AND a.product = b.product
GROUP BY a.month
ORDER BY a.month;

SELECT month, COUNT(*) AS rows_in_actuals
FROM actuals
GROUP BY month;

SELECT month, COUNT(*) AS rows_in_budget
FROM budget
GROUP BY month;

SELECT DISTINCT month, region, product FROM actuals;

SELECT DISTINCT month, region, product FROM budget;

SELECT month, COUNT(*) FROM budget GROUP BY month;

